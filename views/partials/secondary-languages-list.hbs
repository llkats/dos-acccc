<ul class="secondary-list languages list" id="secondary-languages-list">
  {{#each htmlWebpackPlugin.options.data.languageData.secondaryLanguages}}
    <li class="language-item">
      <a href="./more-info/{{enLowercase}}.html" class="usa-button" data-enlang="{{enName}}">
        {{language}}
      </a>
      <small>{{enName}}</small>
    </li>
  {{/each}}
</ul>
