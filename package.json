{
  "name": "hello-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "scripts": {
    "build": "npm run clean && webpack --mode production",
    "build-dev": "webpack --mode development",
    "build-json": "node json-transformer.js data/copy/landing-page-copy.csv data/copy/landing-page-copy.json",
    "clean": "find ./more-info -type f -name '*.html' -exec rm -r {} + && rm -f ./index.html ./main.js",
    "start": "npm run build && webpack-dev-server --mode development"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "csvtojson": "^2.0.10",
    "handlebars": "^4.5.3",
    "list.js": "^1.5.0",
    "uswds": "^2.0.2"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/llkats/dos-acccc"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "handlebars",
    "civic tech"
  ],
  "devDependencies": {
    "csv2json": "^2.0.1",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
